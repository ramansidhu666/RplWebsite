                        @using (Html.BeginForm("Testimonial", "Home", FormMethod.Post, new { enctype = "multipart/form-data" }))
                        {
                            <ul>
                                <li>
                                    <input name="file" type="file" placeholder="Choose file">
                                </li>
                                <li>
                                    <input name="FirstName" type="text" placeholder="Your First Name" required>
                                </li>
                                <li>
                                    <input name="LastName" type="text" placeholder="Your Last Name">
                                </li>
                                <li>
                                    <input name="Email" type="text" placeholder="Your E-mail">
                                </li>
                                <li>
                                    <input name="PhoneNo" type="text" placeholder="Your Phone No">
                                </li>
                                <li>
                                    <input name="Brokerage" type="text" placeholder="Your Brokerage">
                                </li>
                                <li>
                                    <textarea name="Remarks" id="Comment" class="txtarea" placeholder="Description" required></textarea>
                                </li>
                                <li>
                                    <input type="submit" value="Register">
                                </li>
                            </ul>
                        }


                    </div>

                </div>
            </div>
